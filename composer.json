	"require": {
		"php": ">=7.2 <9.0",
		"ext-json": "*",
		"nette/application": "^3.0.1",
		"nette/di": "^3.0.0"
	},
	"require-dev": {
		"nette/bootstrap": "^3.0",
		"latte/latte": "^2.5",
		"tracy/tracy": "^2.6",
