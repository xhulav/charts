	"description": "Integration of Charts.js as Nette Framework component.",
	"keywords": ["charts", "nette"],
	"license": "MIT",
	"prefer-stable": true,
	"minimum-stability": "dev",
	"authors": [
		{
			"name": "Vladimír Hůla",
			"email": "xhulav@gmail.com"
		}
	],
	"autoload": {
		"psr-4": {
			"xhulav\\Charts\\": "src/"
		}
